<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0"
    author=""
    shortdescription="UA:Education"
    uk.shortdescription="UA:Освіта"
    description="Preset for educational institutions in Ukraine"
    uk.description="Заготовки для додавання навчальних закладів в Україні"
    version="0.20_2021-04-29">
    <group name="UA:Education"
        uk.name="UA:Освіта"
        icon="icons/edu.svg">
        <group name="UA:Schools"
            uk.name="Школи"
            icon="icons/school/group.svg"
            icon_size="16">
            <item name="School territory"
                uk.name="Територія школи"
                type="node,closedway,multipolygon"
                icon="icons/school/item.svg"
                icon_size="16"
                preset_name_label="true">
                <link wiki="Uk:Tag:amenity=school" text="Детальніше про теґування шкіл" />
                <key key="amenity"
                    value="school" />
                <text key="name"
                    text="Назва школи"
                    default="" />
                <text key="name:uk"
                    text="Назва школи українською"
                    default="" />
                <text key="official_name"
                    text="Офіційна назва"
                    default="" />
                <text key="short_name"
                    text="Коротка назва"
                    default="" />
                <text key="operator"
                    text="Керуюча установа"
                    default="" />
                <combo key="operator:type"
                    text="Тип установи"
                    values="public,private,government;"
                    display_values="публічна (комунальна),приватна,державна"
                    values_sort="false" />
                <check key="fee"
                    text="Плата за навчання"
                    default="off"
                    delete_if_empty="true" />
                <text key="capacity"
                    text="Макс. кількість учнів"
                    default="" />
                <item_separator />
                <text key="contact:phone"
                    text="Телефон, наприклад +380 44 123 4567"
                    default="" />
                <text key="contact:email"
                    text="Електронна пошта"
                    default="" />
                <text key="contact:website"
                    text="Сайт"
                    default="" />
                <text key="ref:edbo"
                    text="Код школи в реєстрі ЄДЕБО"
                    default="" />
                <text key="ref:isuo"
                    text="Номер школи в системі ІСУО"
                    default="" />
                <item_separator />
                <multiselect key="isced:level"
                    text="Рівень освіти (ISCED)"
                    values="0;1;2;3"
                    rows="5"
                    delete_if_empty="true" />
                <text key="min_age"
                    text="Мінімальний вік учнів"
                    default="" />
                <text key="mах_age"
                    text="Максимальний вік учнів"
                    default="" />
            </item> <!-- School territory -->
            <group name="Territory of the ..."
                uk.name="Територія ..."
                icon="icons/school/area.svg.svg"
                icon_size="16">
                <item name="NVK (І-ІІ)"
                    uk.name="НВК (І-ІІ ступенів)"
                    type="node,closedway,multipolygon"
                    preset_name_label="true">
                    <key key="amenity"
                        value="school" />
                    <key key="isced:level"
                        value="0;1;2" />
                    <key key="min_age"
                        value="3" />
                </item> <!-- NVK_1 territory -->
                <item name="NVK (І-ІІІ)"
                    uk.name="НВК (І-ІІІ ступенів)"
                    type="node,closedway,multipolygon"
                    preset_name_label="true">
                    <key key="amenity"
                        value="school" />
                    <key key="isced:level"
                        value="0;1;2;3" />
                    <key key="min_age"
                        value="3" />
                </item> <!-- NVK_2 territory -->
                <separator />
                <item name="School (І-ІІ)"
                    uk.name="Школи (І-ІІ ступенів)"
                    type="node,closedway,multipolygon"
                    preset_name_label="true">
                    <key key="amenity"
                        value="school" />
                    <key key="isced:level"
                        value="1;2" />
                </item> <!-- School_1 territory -->
                <item name="School (І-ІІІ)"
                    uk.name="Школи (І-ІІІ ступенів)"
                    type="node,closedway,multipolygon"
                    preset_name_label="true">
                    <key key="amenity"
                        value="school" />
                    <key key="isced:level"
                        value="1;2;3" />
                </item> <!-- School_2 territory -->
            </group> <!-- One click territory-->
        </group> <!-- UA:School -->
    </group> <!-- UA:Education -->
</presets>